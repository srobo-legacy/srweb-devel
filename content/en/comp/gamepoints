<table>
    <thead>
        <tr>
            <th>Team</th>
            <th>Points</th>
        </tr>
    </thead>
    <tr data-ng-repeat="item in game_points" id="{{item.tla}}">
        <td>{{item.tla|teamName:teams}}</td>
        <td>{{item.points}}</td>
    </tr>
</table>
